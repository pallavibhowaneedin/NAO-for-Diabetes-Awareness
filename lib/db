CREATE DATABASE APP_DATABASE;
USE APP_DATABASE;

CREATE TABLE SteamEffluent2(
	DateAndTime DATETIME,
	AEL_SERVICE_WATER FLOAT,
	AML_SERVICE_WATER FLOAT,
	EFFLUENT_TO_SANGUI FLOAT,
	EFFLUENT_FROM_ARL FLOAT,
	ARL_INDUSTRIAL_WATER FLOAT,
	EFFLUENT_BEHIND_WORKSHOP FLOAT,
	EFFLUENT_FROM_JTA_WEIR_IRRIGATION FLOAT,
	EFFLUENT_FROM_FCB_WEIR_AEL FLOAT,
	WATER_FROM_MILL_BEARINGS FLOAT,
	EFFLUENT_COOLING_TOWER_CANAL FLOAT,
	EFFLUENT_FROM_ARL_WEIR FLOAT,
	LAVAGE_MOULINS FLOAT,
	OVERFLOW_COOLING_TOWER FLOAT,

	STEAM_BP FLOAT,
	ABB_COND_BP FLOAT,
	YOKOGAWA_COND_BP FLOAT,
	FLOW_V1 FLOAT,
	ENERGY_V1 FLOAT,
	MIXED_JUICE_FLOW_TOTALISER FLOAT,
	MIXED_JUICE_SCALE_TOTALISER FLOAT,

	CONDITIONING_SILO_2 FLOAT,
	CENTRIFUGALS FLOAT,
	CHILLER FLOAT,
	COOLING_TOWER FLOAT,
	CONDITIONING_SILO_1 FLOAT,
	PANS FLOAT,
	LIQUOR_FEED FLOAT,
	REMELTER FLOAT,
	SUGAR FLOAT,
	EFFLUENT_FROM_ALTEO FLOAT, 
	DILUTED_EFFLUENT FLOAT,
	SERVICE_WATER FLOAT,
	EFFLUENT_TO_AQUATAN FLOAT,
	HOMOGENISING_TANK FLOAT,
	CAUSTIC_RINSE_TO_HOMO FLOAT,
	CAUSTIC_WASH_TO_HOMO FLOAT,
	XS_TO_HOMO FLOAT
);

ALTER TABLE Energy
ADD 

CREATE TABLE Energy2(
DateAndTime DATETIME,
METER_READING_PS1	FLOAT,
METER_READING_PS2	FLOAT,
METER_READING_EXP_ FLOAT,
METER_READING_DJTL1 FLOAT,
METER_READING_DJTL3 FLOAT,
METER_READING_TM1	FLOAT,
METER_READING_TM2	FLOAT,
METER_READING_TR9	FLOAT,
METER_READING_TR2A FLOAT,
METER_READING_TR2B FLOAT,
METER_READING_TR5	FLOAT,
METER_READING_TR3A FLOAT,
METER_READING_TR7A FLOAT,
METER_READING_TR6	FLOAT,
METER_READING_TR8	FLOAT,
METER_READING_SHRED FLOAT,
METER_READING_SHRED_MOT FLOAT,
DJTR1 FLOAT,
DJTR2 FLOAT,
DJTR3 FLOAT,
NON_SECOURS_IT FLOAT,
NON_SECOURS_GARAGE FLOAT,
SECOURS_BUREAU_USINE FLOAT,
SECOURS_ATELIER	FLOAT,
SECOURS_GARAGE FLOAT,
SECOURS_IT FLOAT
);

SELECT * FROM SteamEffluent2 ORDER BY DateAndTime ASC;
SELECT * FROM Energy2 ORDER BY DateAndTime ASC;

SELECT *
FROM SteamEffluent
WHERE CAST(DateAndTime AS TIME) = '00:00:00'; /select only midnight time from steameffluent, change for 06 00 if necessary/

SELECT *
FROM SteamEffluent
WHERE CAST(DateAndTime AS TIME) = '06:00:00';

SELECT *
FROM Energy
WHERE CAST(DateAndTime AS TIME) = '00:00:00'; /select only midnight time from energy, change for 06 00 if necessary/

SELECT *
FROM Energy
WHERE CAST(DateAndTime AS TIME) = '06:00:00';

SELECT * FROM SteamEffluent ORDER BY DateAndTime ASC;
SELECT * FROM Energy ORDER BY DateAndTime ASC;

ALTER TABLE SteamEffluent1Friday ADD DateAndTime DATETIME

DELETE FROM Energy
DELETE FROM SteamEffluent 


